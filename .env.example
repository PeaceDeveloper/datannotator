# Label Studio Configuration
# Admin user credentials for Label Studio
LABEL_STUDIO_USERNAME=admin
LABEL_STUDIO_PASSWORD=your_secure_password_here
LABEL_STUDIO_USER_TOKEN=your_api_token_here

# ngrok Configuration
# Get your auth token from https://dashboard.ngrok.com/get-started/your-authtoken
NGROK_AUTHTOKEN=your_ngrok_authtoken_here

# ngrok region (optional, defaults to 'us')
# Available regions: us, eu, ap, au, sa, jp, in
NGROK_REGION=us


CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here
CLOUDFLARE_TUNNEL_NAME=label-studio-tunnel